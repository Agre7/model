  "keywords": [
    "functional",
    "reactive",
    "visualization"
  ],
  "author": "curran <curran.kelleher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curran/model/issues"
  },
  "homepage": "https://github.com/curran/model",
  "devDependencies": {
    "body-parser": "^1.2.0",
    "chai": "^1.9.1",
    "docco": "^0.7.0",
    "express": "^4.0.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^0.1.5",
    "mocha": "^2.3.4",
    "requirejs": "^2.1.11",
    "uglify-js": "^2.6.1"
  }
